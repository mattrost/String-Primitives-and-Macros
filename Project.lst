				; (insert macro definitions here)
				mGetString MACRO prompt, user_input, input_length

					PUSH		EDX
					PUSH		ECX
					PUSH		EAX

					MOV			EDX, prompt
					CALL		WriteString

					MOV			EDX, user_input
					MOV			ECX, HI

					CALL		ReadString
					MOV			input_length, EAX

					POP			EAX
					POP			ECX
					POP			EDX
				ENDM

				; mDisplayString based off of MacroDemo.asm from Module 8 - Exploration 2
				mDisplayString MACRO string
					PUSH		EDX

					MOV			EDX, string
					CALL		WriteString

					POP			EDX

				ENDM

